       RowBox[{"c", "[", "m", "]"}], "=", 
       RowBox[{"Binomial", "[", 
        RowBox[{"mEuler", ",", 
         RowBox[{"m", "-", "1"}]}], "]"}]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"uu", "=", 
      RowBox[{
       RowBox[{"Exp", "[", 
        RowBox[{"aa", "/", "2"}], "]"}], "/", "t"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"xx", "=", 
      RowBox[{"aa", "/", 
       RowBox[{"(", 
        RowBox[{"2", "*", "t"}], ")"}]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"suma", "=", 
      RowBox[{
       RowBox[{
        RowBox[{"Re", "[", 
         RowBox[{"N", "[", 
          RowBox[{
           RowBox[{"funs", "[", 
            RowBox[{"xx", ",", "argu"}], "]"}], ",", "preci"}], "]"}], "]"}], 
        "/", "2"}], "+", 
       RowBox[{"Sum", "[", 
        RowBox[{
         RowBox[{
          RowBox[{
           RowBox[{"(", 
            RowBox[{"-", "1"}], ")"}], "^", "nn"}], "*", 
          RowBox[{"Re", "[", 
           RowBox[{"N", "[", 
            RowBox[{
             RowBox[{"funs", "[", 
              RowBox[{
               RowBox[{"xx", "+", 
                RowBox[{"I", "*", "nn", "*", "hh"}]}], ",", "argu"}], "]"}], 
             ",", "preci"}], "]"}], "]"}]}], ",", 
         RowBox[{"{", 
          RowBox[{"nn", ",", "1", ",", "ntr"}], "}"}]}], "]"}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"su", "[", "1", "]"}], "=", "suma"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Do", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"nn", "=", 
         RowBox[{"ntr", "+", "k"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"su", "[", 
          RowBox[{"k", "+", "1"}], "]"}], "=", 
         RowBox[{"N", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"su", "[", "k", "]"}], "+", 
            RowBox[{
             RowBox[{
              RowBox[{"(", 
               RowBox[{"-", "1"}], ")"}], "^", "nn"}], "*", 
             RowBox[{"Re", "[", 
              RowBox[{"N", "[", 
               RowBox[{
                RowBox[{"funs", "[", 
                 RowBox[{
                  RowBox[{"xx", "+", 
                   RowBox[{"I", "*", "nn", "*", "hh"}]}], ",", "argu"}], 
                 "]"}], ",", "preci"}], "]"}], "]"}]}]}], ",", "preci"}], 
          "]"}]}]}], ",", 
       RowBox[{"{", 
        RowBox[{"k", ",", "1", ",", 
         RowBox[{"mEuler", "+", "1"}]}], "}"}]}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Do", "[", 
      RowBox[{
