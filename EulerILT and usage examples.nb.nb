Cell[BoxData[
 RowBox[{
  RowBox[{"EulerILTwithTruncationError", "[", 
   RowBox[{"funs_", ",", "t_", ",", "argu_List", ",", 
    RowBox[{"preci_", ":", "$MachinePrecision"}]}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"aa", "=", "50"}], ",", 
      RowBox[{"ntr", "=", "150"}], ",", 
      RowBox[{"mEuler", "=", "40"}], ",", 
      RowBox[{"(*", " ", 
       RowBox[{
        RowBox[{"aa", "=", "30"}], ",", 
        RowBox[{"ntr", "=", "100"}], ",", 
        RowBox[{"mEuler", "=", 
         RowBox[{"30", " ", "are", "  ", "default", " ", "values"}]}]}], " ", 
       "*)"}], "\[IndentingNewLine]", 
      RowBox[{"hh", "=", 
       RowBox[{"Pi", "/", "t"}]}], ",", "suma", ",", 
      RowBox[{"avgsu", "=", "0"}], ",", 
      RowBox[{"avgsu1", "=", "0"}], ",", "fun", ",", "fun1", ",", "c", ",", 
      "xx", ",", "su", ",", "uu", ",", "nn", ",", "errt"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{
      RowBox[{"c", "[", "m_", "]"}], ":=", 
